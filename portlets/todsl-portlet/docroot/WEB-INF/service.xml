<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">

<service-builder package-path="com.liferay.todsl" auto-namespace-tables="false">
	<author>Ryan Schuhler</author>
	<namespace>TODSL</namespace>
	<entity name="Trip" local-service="true" remote-service="true">

		<!-- PK fields -->

		<column name="tripId" type="long" primary="true" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Creation fields -->

		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />

		<!-- Basic fields -->

		<column name="tripCapacity" type="int" />
		<column name="tripCostEstimate" type="String" />
		<column name="tripDescription" type="String" />
		<column name="tripEnd" type="Date" />
		<column name="tripFriendlyUrl" type="String" />
		<column name="tripHost" type="String" />
		<column name="tripImage" type="String" />
		<column name="tripLocation" type="String" />
		<column name="tripStart" type="Date" />
		<column name="tripTitle" type="String" />
		<column name="tripTotalCost" type="double" />

		<!-- Table fields -->

		<column name="displayTripCountdown" type="boolean" />
		<column name="displayTripDiscussion" type="boolean" />
		<column name="displayTripExpenses" type="boolean" />
		<column name="displayTripGearItems" type="boolean" />
		<column name="displayTripGearGroupItems" type="boolean" />
		<column name="displayTripGearLendingItems" type="boolean" />
		<column name="displayTripMap" type="boolean" />
		<column name="displayTripTransportation" type="boolean" />
		<column name="displayTripWeather" type="boolean" />

		<!-- Relational fields -->

		<column name="tripExpenses" entity="TripExpense" mapping-table="Trip_TripExpense" type="Collection" />
		<column name="tripGearGroupItems" entity="TripGearGroupItem" mapping-table="Trip_TripGearGroupItem" type="Collection" />
		<column name="tripGearItems" entity="TripGearItem" mapping-table="Trip_TripGearItem" type="Collection" />
		<column name="tripGearLendingItems" entity="TripGearLendingItem" mapping-table="Trip_TripGearLendingItem" type="Collection" />
		<column name="tripMembers" entity="TripMember" mapping-table="Trip_TripMember" type="Collection" />
		<column name="tripTransportations" entity="TripTransportation" mapping-table="Trip_TripTransportation" type="Collection" />
	</entity>
	<entity name="TripExpense" local-service="true" remote-service="true">

		<!-- PK fields -->

		<column name="tripExpenseId" type="long" primary="true" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />
		<column name="tripId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />

		<!-- Other fields -->

		<column name="tripExpenseCost" type="double" />
		<column name="tripExpensePayeeMemberId" type="long" />
		<column name="tripExpenseTitle" type="String" />

		<!-- Relational fields -->

		<column name="trips" entity="Trip" mapping-table="Trip_TripExpense" type="Collection" />

		<!-- Finder methods -->

		<finder name="Trip" return-type="Collection">
			<finder-column name="tripId"></finder-column>
		</finder>

		<finder name="TripMember" return-type="Collection">
			<finder-column name="tripId"></finder-column>
			<finder-column name="tripExpensePayeeMemberId"></finder-column>
		</finder>
	</entity>
	<entity name="TripGearItem" local-service="true" remote-service="true">

		<!-- PK fields -->

		<column name="tripGearItemId" type="long" primary="true" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />
		<column name="tripId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />

		<!-- Other fields -->

		<column name="itemCategory" type="String" />
		<column name="itemRequired" type="boolean" />
		<column name="itemTitle" type="String" />

		<!-- Relational fields -->

		<column name="trips" entity="Trip" mapping-table="Trip_TripGearItem" type="Collection" />

		<!-- Finder methods -->

		<finder name="Trip" return-type="Collection">
			<finder-column name="tripId"></finder-column>
		</finder>

		<finder name="TripRequired" return-type="Collection">
			<finder-column name="tripId"></finder-column>
			<finder-column name="itemRequired"></finder-column>
		</finder>
	</entity>
	<entity name="TripGearGroupItem" local-service="true" remote-service="true">

		<!-- PK fields -->

		<column name="tripGearGroupItemId" type="long" primary="true" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />
		<column name="tripId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />

		<!-- Other fields -->

		<column name="itemCategory" type="String" />
		<column name="itemQuantity" type="int" />
		<column name="itemRequired" type="boolean" />
		<column name="itemTitle" type="String" />

		<!-- Relational fields -->

		<column name="itemClaimTripMembers" entity="TripMember" mapping-table="TripMember_TripGearGroupItem" type="Collection" />
		<column name="trips" entity="Trip" mapping-table="Trip_TripGearGroupItem" type="Collection" />

		<!-- Finder methods -->

		<finder name="Trip" return-type="Collection">
			<finder-column name="tripId"></finder-column>
		</finder>
	</entity>
	<entity name="TripGearLendingItem" local-service="true" remote-service="true">

		<!-- PK fields -->

		<column name="tripGearLendingItemId" type="long" primary="true" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />
		<column name="tripId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />

		<!-- Other fields -->

		<column name="itemBorrowerMemberId" type="long" />
		<column name="itemLenderMemberId" type="long" />
		<column name="itemTitle" type="String" />

		<!-- Relational fields -->

		<column name="trips" entity="Trip" mapping-table="Trip_TripGearLendingItem" type="Collection" />

		<!-- Finder methods -->

		<finder name="Trip" return-type="Collection">
			<finder-column name="tripId"></finder-column>
		</finder>

		<finder name="TripBorrower" return-type="Collection">
			<finder-column name="tripId"></finder-column>
			<finder-column name="itemBorrowerMemberId"></finder-column>
		</finder>

		<finder name="TripLender" return-type="Collection">
			<finder-column name="tripId"></finder-column>
			<finder-column name="itemLenderMemberId"></finder-column>
		</finder>
	</entity>
	<entity name="TripMember" local-service="true" remote-service="true">

		<!-- PK fields -->

		<column name="tripMemberId" type="long" primary="true" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />
		<column name="tripId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />

		<!-- Other fields -->

		<column name="tripMemberInvitedByMemberId" type="long" />
		<column name="tripMemberEmail" type="String" />
		<column name="tripMemberName" type="String" />
		<column name="tripMemberUserId" type="long" />
		<column name="tripMemberStatus" type="int" />
		<column name="tripMemberTotalExpenses" type="double" />

		<!-- Relational fields -->

		<column name="trips" entity="Trip" mapping-table="Trip_TripMember" type="Collection" />
		<column name="tripTransportations" entity="TripTransportation" mapping-table="TripMember_TripTransportation" type="Collection" />
		<column name="tripGearGroupItems" entity="TripGearGroupItem" mapping-table="TripMember_TripGearGroupItem" type="Collection" />

		<!-- Finder methods -->

		<finder name="Trip" return-type="Collection">
			<finder-column name="tripId"></finder-column>
		</finder>
		<finder name="TripInvite" return-type="Collection">
			<finder-column name="tripId"></finder-column>
			<finder-column name="tripMemberInvitedByMemberId"></finder-column>
		</finder>
		<finder name="TripMember" return-type="Collection">
			<finder-column name="tripId"></finder-column>
			<finder-column name="tripMemberUserId"></finder-column>
		</finder>
		<finder name="TripMemberEmail" return-type="Collection">
			<finder-column name="tripId"></finder-column>
			<finder-column name="tripMemberEmail"></finder-column>
		</finder>
		<finder name="TripStatus" return-type="Collection">
			<finder-column name="tripId"></finder-column>
			<finder-column name="tripMemberStatus"></finder-column>
		</finder>
		<finder name="User" return-type="Collection">
			<finder-column name="userId"></finder-column>
		</finder>
	</entity>
	<entity name="TripTransportation" local-service="true" remote-service="true">

		<!-- PK fields -->

		<column name="tripTransportationId" type="long" primary="true" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />
		<column name="tripId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />

		<!-- Other fields -->

		<column name="capacity" type="int" />
		<column name="driverMemberId" type="long" />

		<!-- Relational fields -->

		<column name="trips" entity="Trip" mapping-table="Trip_TripTransportation" type="Collection" />
		<column name="tripMembers" entity="TripMember" mapping-table="TripMember_TripTransportation" type="Collection" />

		<!-- Finder methods -->

		<finder name="Trip" return-type="Collection">
			<finder-column name="tripId"></finder-column>
		</finder>

		<finder name="TripDriver" return-type="Collection">
			<finder-column name="tripId"></finder-column>
			<finder-column name="driverMemberId"></finder-column>
		</finder>
	</entity>
</service-builder>