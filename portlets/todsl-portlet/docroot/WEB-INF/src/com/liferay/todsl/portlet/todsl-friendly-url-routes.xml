<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.2.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_6_2_0.dtd">

<routes>
	<route>
		<pattern>/all</pattern>
		<implicit-parameter name="mvcPath">/view_trips.jsp</implicit-parameter>
	</route>
	<route>
		<pattern>/create</pattern>
		<implicit-parameter name="mvcPath">/create.jsp</implicit-parameter>
	</route>
	<route>
		<pattern>/{tripId:\d+}</pattern>
	</route>
	<route>
		<pattern>/{tripId:\d+}/edit</pattern>
		<implicit-parameter name="mvcPath">/edit.jsp</implicit-parameter>
	</route>
	<route>
		<pattern>/{tripId:\d+}/rsvp/{tripMemberId:\d+}</pattern>
		<implicit-parameter name="mvcPath">/rsvp.jsp</implicit-parameter>
	</route>
	<route>
		<pattern>/{tripId:\d+}/{appName}</pattern>
	</route>
	<route>
		<pattern>/{tripName}</pattern>
	</route>
	<route>
		<pattern>/{tripName}/edit</pattern>
		<implicit-parameter name="mvcPath">/edit.jsp</implicit-parameter>
	</route>
	<route>
		<pattern>/{tripName}/rsvp/{tripMemberId:\d+}</pattern>
		<implicit-parameter name="mvcPath">/rsvp.jsp</implicit-parameter>
	</route>
	<route>
		<pattern>/{tripName}/{appName}</pattern>
	</route>
</routes>